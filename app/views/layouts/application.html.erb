<!DOCTYPE html>
<html>
  <head>
    <title>VulnerableGems</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet">
  </head>

  <body class="content">

    <header class="full-width fixed">
      <div class="logo-container">
      <img src="/assets/logo.png">
    </header>

    <div class="main">
      <%= yield %>
    </div>

    <footer class="full-width">
      <div class="logo-container">
        <h1 class="logo">
          <a href="https://www.ombulabs.com"> <img src="/assets/logo.png"></a>
      </div>
      <nav class="menu">
        <a href="/#products">Products</a>
        <a href="/#process">Our process</a>
        <a href="/#open-source">Open Source</a>
        <a href="/#staff">Team</a>
        <a href="/jobs">Jobs</a>
        <a href="/blog">Blog</a>
        <a href="/contact">Contact Us</a>
      </nav>
    </footer>

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </body>
</html>
